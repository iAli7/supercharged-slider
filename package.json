{
  "name": "supercharged-slider",
  "version": "0.1.0",
  "description": "A starter for build library with Typescript with the fast Vite and all static code testing with Eslint and formatting with Prettier.",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint ./src --fix",
    "*.scss": "stylelint -s --config-basedir --fix"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/dom": "^8.16.0",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "rollup-plugin-typescript-paths": "^1.3.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^14.1.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "vite": "^2.1.0",
    "vite-plugin-dts": "^1.4.0"
  },
  "pre-commit": "lint",
  "files": [
    "dist"
  ],
  "keywords": [],
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "exports": {
    ".": {
      "import": "./dist/main.ejs.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "typings": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/necatikcl/supercharged-slider.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/necatikcl/supercharged-slider/issues"
  },
  "homepage": "https://github.com/necatikcl/supercharged-slider#readme"
}
